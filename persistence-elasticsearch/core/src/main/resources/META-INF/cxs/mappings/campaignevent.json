{
  "dynamic_templates": [
    {
      "all": {
        "match": "*",
        "match_mapping_type": "string",
        "mapping": {
          "type": "text",
          "analyzer": "folding",
          "fields": {
            "keyword": {
              "type": "keyword",
              "ignore_above": 256
            }
          }
        }
      }
    }
  ],
  "properties": {
    "cost": {
      "type": "double"
    },
    "eventDate": {
      "type": "date"
    },
    "metadata": {
      "properties": {
        "enabled": {
          "type": "boolean"
        },
        "hidden": {
          "type": "boolean"
        },
        "missingPlugins": {
          "type": "boolean"
        },
        "readOnly": {
          "type": "boolean"
        }
      }
    }
  }
}